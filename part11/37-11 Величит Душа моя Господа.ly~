PartPOneVoiceTwo =  \relative bes' {
    \clef "treble" \time 10/4 \key bes \major s1*10 s2*15 s1*5 |
    \barNumberCheck #10
    bes2 ( ~ bes8 [ a8 ] bes2. \bar "||"
    s1 s2*25 s2*15 s1*5 | % 21
    bes2 ~ bes8 [ a8 ] bes2. s2*17 s1*10 s4*13 as16 [ f16 ] s8*13 s8*11
    g8 g8 g8 s2. | % 32
    bes2 ~ bes8 [ a8 ] bes2. s2*7 | % 34
    es4 f4 ) s1*2 s2*25 s1*10 s1*5 s4*5 bes,8 bes8 s1 | % 47
    bes2 ( ~ bes8 [ a8 ] bes2. s1*6 s1*10 s2*5 | % 55
    g'4 f4 es4 ) d4 s2*23 s1*5 s2*9 bes8 bes8 s4 | % 64
    bes4. ( a8 bes8 [ a8 ] g2. \bar "|."
    }

PartPTwoVoiceOne =  \relative d' {
    \clef "treble" \time 10/4 \key bes \major | % 1
    d4 -\p -
    "Bе – ли      –       чит           Ду-ша  Мо– я,                       Бе  –  ли          –                чит            Ду  –  ша"
    -
    "Ve – li        –       chit           Du-sha Mo-ya,                      Be  –  li            –                chit            Du  – sha"
    s4*9 | % 2
    d2. d2 d4 d4 -- -\> <c d>4 -- <bes d>4 ( ~ -- s4 -\! | % 3
    <bes d>2 ) d8 [ -\mf f8 ] g2. s1 | % 4
    f2 -\< g8 [ bes8 ] c2 ( -\f -\> bes4 s1 -\! -\! | % 5
