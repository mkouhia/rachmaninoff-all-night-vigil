    <bes, bes'>2. -- bes'2. -\pp s1 | % 22
    bes'4 -\f -
    "Я  –   ко      сотвори  Мне   ве    –      ли  –  чи – е           Силь  –  ный, и    свя – то"
    -
    "Ya –   ko      sotvori     Mne   ve    –      li    –   chi –ye          Sil'     –  ny,     i    svya –toi"
    bes8 [ g8 ] a8 bes8 c4 bes4 a4 s1 | % 23
    g4 f4 -\< g4 s4*7 -\! | % 24
    as4 ( -- g8 [ f8 ) ] es4 es4 -\mf f4 -\< g4 s1 -\! | % 25
    g4 -- -
    "и– мя   Е  –   го, и милость Е– го в ро-ды    родов   бо  –  я –щим-ся            Е–го."
    -
    "i – mya Ye – vo,    i  milost'  Ye–vo v ro-dyi   podov   bo–ya–shchim-sya       Ye-vo."
    -\> f8 [ es8 ] d4 d2 -- -\p d4 -\mf d4 c4 -\< d4 s4 -\! -\! | % 26
    es4 -- d8 -- [ -\> c8 -- ] bes4 -- c8 -- [ -\> bes8 -- ] a4 -- g4
    -\< s1 -\! -\! -\! | % 27
    as4 ( -- g4 ) f4 es8 ( [ f8 ] g4 ) -\> g4 as2 -- -\pp r4 s4 -\! | % 28
    r1 s1. | % 29
    r1 r2 r4 r8 s8*5 | \barNumberCheck #30
    r1 r2 s1 | % 31
    es'8 [ -\p f8 ] es8 [ d8 ] c8 [ bes16 c16 ] d8 -\> d8 g,8 -\pp g8 g8
    g8 -- g8 -- g8 -- s2. -\! | % 32
    <bes, bes'>2. -- bes'2. -\pp s1 | % 33
    bes'8 g8 s4*9 | % 34
    es'4 ( -\ff d4 ) c4 g8 bes8 s1. | % 35
    c4 bes4 as4 es8 [ -\> g8 ] s1. -\! | % 36
    as4 -\mf g4 f4 -\< g4 s1. -\! | % 37
    g2 -\> g4 g4 s1. -\! | % 38
    f4 -\p <es f>4 <d f>4 -\< <c es>8 <bes es>8 s1. -\! | % 39
    <as es'>4 -- -\mf <bes es>4 -\> <c es>8 <c es>8 <c es>8 es8 s1. -\!
    | \barNumberCheck #40
    d4 -\pp d4 d4 d4 s1. -\! | % 41
    <g, d'>4 -\p <a d>4 -\> bes8 ( [ c8 ] d4 ) s1. -\! | % 42
    es2 -\ppp r4 s4*7 | % 43
    r1 s1. | % 44
    r1 r2 r4 r8 s8*5 | % 45
    r1 r2 s1 | % 46
    c'8 [ -\p g8 ] es8 -\> d8 c8 f8 bes,8 -\pp bes8 bes8 bes8 bes8 --
    bes8 -- s1 -\! | % 47
    <bes, bes'>2. bes'2. -\pp s1 | % 48
    <g d'>4 -\pp -\markup{ \italic {Slightly emphasizing the lower
            voice} } -\markup{ \italic {Немного выдвигая нижний голос} }
    - "Вос-при-ят     Из-ра-ил  – я" -
    "Vos–pri–yat     Iz –ra– il   – ya" <f d'>4 s1*2 | % 49
    d'2 d4 d4 d4 d4 s1 | \barNumberCheck #50
    d4 -
    "от –ро–ка Сво   –     е   –  го,  по–мя–ну    –    ти       ми–ло– сти,"
    -
    "ot – ro –ka Svo    –    ye  – vo,  po-mya-nu  –     ti        mi–lo – sti,"
    f4 d4 d2 d4 s1 | % 51
    f4 d4 d4 g2 f4 s1 | % 52
    <c es>4 <bes f'>4 <g es'>4 ( ~ <bes es>4 ) c8 -
    "я – ко–же гла– го – ла     ко от-цем наш-им," -
    "ya–ko-zhe gla –go – la      ko ot-tsem nash-im," bes8 -\f bes8 c8
    s1 | % 53
    es4 c4 d8 es8 f4 d4 f8 [ g8 ] s1 | % 54
    g4 ( -
    "Ав    –    pа  – а  –  му   и              се        –        ме   –   ни"
    -
    "Av    –     ra  –  a  – mu   i              se        –        me  –    ni"
    - "е      –   го," - "ye    –   vo," f4 ) g8 [ bes8 ] bes4 a8 [ bes8
    ] d4 s1 | % 55
    g,2. -\ff g4 s1. | % 56
    g2. d8 [ -\f f8 ] s1. | % 57
    g2. g4 - "до       ве    –     ка." - "do       ve    –     ka." s1.
    | % 58
    g4 ( -\pp f4 ) es4 ( d4 ~ s1. | % 59
    d2 ) \fermata r2 s1. | \barNumberCheck #60
    r1 s1. | % 61
    r1 r2 r4 r8 s8*5 | % 62
    r1 r2 s1 | % 63
    g4 -\p g4 g8 [ f8 ] es8 -\< d8 c4. ( -\> f8 bes,8 ) bes8 -\pp bes8
    bes8 bes8 -- bes8 -- s4 -\! -\! | % 64
    <bes, bes'>4. ( -\p <c c'>8 -\> <d d'>4 ) <d d'>2. \fermata -\pp
    \bar "|."
    -\! }

PartPFourVoiceTwo =  \relative f {
    \clef "bass" \time 10/4 \key bes \major s2*15 | % 4
    f4 es4 d4 c4 ( bes8 [ a8 ) ] g4 s1 | % 5
    d'4 d4 d4 g,2 s4*25 s2*13 \bar "||"
    s1 s1*10 | % 15
    g'4 ( f4 es4 ) d4 ( c4 ) s4*5 s2*15 s1*5 s1*10 s1*10 s1*5 s1*10
    s4*13 es8 [ d8 ] s1. | % 37
    c4 ( d4 ) es4 s4*27 | \barNumberCheck #40
    r4 r4 bes8 [ c8 ] -\< bes8 [ a8 ] s1*9 s1*5 s2*15 | % 49
    bes4 ( a4 ) bes4 c4 bes8 [ c8 ] a4 s1 | \barNumberCheck #50
    bes4 a4 bes4 a4 ( g8 [ a8 ) ] f4 s1 | % 51
    g4 a4 bes4 d4 ( c8 [ d8 ) ] bes4 s1*6 s1*5 | % 56
    g'2. d4 s1. | % 57
    g,2. d8 [ f8 ] s1. | % 58
    g2 g2 ~ s1. | % 59
    g2 \fermata s1*2 s1*5 s1*5 s1. \bar "|."
    }


% The score definition
\score {
    <<
        
        \new StaffGroup
        <<
            \new Staff
            <<
                \set Staff.instrumentName = "Сопрано"
                \set Staff.shortInstrumentName = "С."
                
                \context Staff << 
                    \mergeDifferentlyDottedOn\mergeDifferentlyHeadedOn
                    \context Voice = "PartPOneVoiceOne" {  \voiceOne \PartPOneVoiceOne }
                    \context Voice = "PartPOneVoiceTwo" {  \voiceTwo \PartPOneVoiceTwo }
                    >>
                >>
            \new Staff
            <<
                \set Staff.instrumentName = "Альтъ"
                \set Staff.shortInstrumentName = "А."
                
                \context Staff << 
                    \mergeDifferentlyDottedOn\mergeDifferentlyHeadedOn
                    \context Voice = "PartPTwoVoiceOne" {  \voiceOne \PartPTwoVoiceOne }
                    \context Voice = "PartPTwoVoiceTwo" {  \voiceTwo \PartPTwoVoiceTwo }
                    >>
                >>
            \new Staff
            <<
                \set Staff.instrumentName = "Теноръ"
                \set Staff.shortInstrumentName = "Т."
                
                \context Staff << 
                    \mergeDifferentlyDottedOn\mergeDifferentlyHeadedOn
                    \context Voice = "PartPThreeVoiceOne" {  \voiceOne \PartPThreeVoiceOne }
                    \context Voice = "PartPThreeVoiceTwo" {  \voiceTwo \PartPThreeVoiceTwo }
                    \context Voice = "PartPThreeVoiceThree" {  \voiceThree \PartPThreeVoiceThree }
                    >>
                >>
            \new Staff
            <<
                \set Staff.instrumentName = "Басъ"
                \set Staff.shortInstrumentName = "Б."
                
                \context Staff << 
                    \mergeDifferentlyDottedOn\mergeDifferentlyHeadedOn
                    \context Voice = "PartPFourVoiceOne" {  \voiceOne \PartPFourVoiceOne }
                    \context Voice = "PartPFourVoiceTwo" {  \voiceTwo \PartPFourVoiceTwo }
                    >>
                >>
            
            >>
        
        >>
    \layout {}
    % To create MIDI output, uncomment the following line:
    %  \midi {\tempo 4 = 40 }
    }

